source leaprc.protein.ff14SB
source leaprc.gaff2

loadAmberParams LIG.frcmod
LIG_TEMPLATE = loadMol2 LIG.mol2

loadAmberParams zn.frcmod
ZN_TEMPLATE = loadMol2 zn.mol2

# Load complex and let tleap add hydrogens
COM = loadPdb complex.pdb

# Check the unit
check COM
charge COM

# Try to save - this will show if there are missing parameters
saveAmberParm COM complex.prmtop complex.inpcrd

quit
