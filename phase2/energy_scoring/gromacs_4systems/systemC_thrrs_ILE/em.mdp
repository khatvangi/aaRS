; Energy minimization parameters
integrator  = steep
emtol       = 1000.0
emstep      = 0.01
nsteps      = 50000

; Neighbor searching
nstlist     = 10
cutoff-scheme = Verlet
ns_type     = grid
rlist       = 1.2

; Electrostatics
coulombtype = PME
rcoulomb    = 1.2

; VdW
vdwtype     = Cut-off
rvdw        = 1.2

; Constraints
constraints = none